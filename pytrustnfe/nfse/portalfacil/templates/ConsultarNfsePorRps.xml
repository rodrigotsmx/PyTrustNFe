            <Cnpj>{{ nfse.cnpj_prestador }}</Cnpj>
        </CpfCnpj>
        <InscricaoMunicipal>{{ nfse.inscricao_municipal }}</InscricaoMunicipal>
    </Prestador>
</ConsultarNfseRpsEnvio>
