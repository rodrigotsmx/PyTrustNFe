<ConsultarNfseRpsEnvio xmlns="{{ base_url }}/nfseserv/schema/nfse_v202.xsd">
    <IdentificacaoRps>
        <Numero>{{nfse.rps.numero}}</Numero>
        <Serie>{{nfse.rps.serie}}</Serie>
        <Tipo>{{nfse.rps.tipo}}</Tipo>
